select job from emp where empno=7369;
--join(조인)
--81p
select empno, ename, deptno from emp;

select empno, deptno from emp;

select deptno from emp;
--4-6
select distinct deptno from emp;

select distinct job, deptno from emp;

select count(*) from emp;

select job, deptno from emp;
--AS
select ename, sal, sal*12+comm as annsal, comm from emp;
select * from emp order by sal desc;
--4-14
select * from emp order by deptno asc, sal desc;
--5-2
select * from emp where deptno = 30;
-- AND, OR(논리연산자)

select * from emp where deptno=30 and job='SALESMAN';
select * from emp where deptno=30 or job='CLERK';

--부서번호가 20 이거나 직업이 SALESMAN인 사원 정보를 출력하시오.
select * from emp where deptno=20 or job='SALESMAN';

--5-5
select * from emp where sal*12 = 36000;

select * from emp where sal >= 3000;

--급여가 2500 이상이고 직업이 ANALYST인 사원
select * from emp where sal >= 2500 and job = 'ANALYST';

--5-7
select * from emp where ename >='F'; -- F~Z

select * from emp where ename <>'FORZ';  --A~F

select * from emp where sal ^= 3000;

--NOT
select * from emp where not sal = 3000;
--IN
select * from emp where job = 'MANAGER'
or job = 'SALESMAN'
or job = 'CLERK';

select * from emp where job IN ('MANAGER','SALESMAN','CLERK');

select * from emp 
    where job NOT IN ('MANAGER','SALESMAN','CLERK');

select * from emp where deptno in (10, 20);

select * from emp where sal >= 2000 and sal <= 3000;
select * from emp where sal between 2000 and 3000;

select * from emp where sal >= 2000 and sal <= 3000;

--5-20
select * from emp where ename = 'S'; -- 이름이 S 인사람을 의미함
select * from emp where ename like 'S%'; --이름이 S로 시작하는 사람을 가져오기

select * from emp where ename like '_L%'; --언더스코어(_) 넣어서 두번째 글자가 L로 시작하는 사람 가져오기

--5-22 '%AM%'
select * from emp where ename like '%AM%'; -- 이름에 AM이 들어간 사람 출력 *검색할때 유용함
select * from emp where ename NOT like '%AM%';

--NULL -> O(X)
select * from emp;

--5-26 is null(데이터 평균값을 구할 때 데이터전처리용도로 사용)
select * from emp where comm is null;
select * from emp where comm is NOT null;
