import smhrd.oop1.A;
import smhrd.oop1.B;

public class OOP53 {

	public static void main(String[] args) {
		// A, B를 저장할 배열을 생성하시오.
		Object[] obj = new Object[2]; // 다형성배열
		obj[0] = new A();
		obj[1] = new B();
		
		disPtr(obj);

	}
	//매개변수로 A,B 클래스를 받아서 display() 메서드를 실행하는 [메서드 정의 = disPtr()]
	public static void disPtr(Object[] obj) {
		for (int i = 0; i < obj.length; i++) {
			if (obj[i] instanceof A) {
				((A) obj[i]).display();
			} else {
				((B) obj[i]).display();
			}
		}

	}

}

