
public class Method02 {

	public static void main(String[] args) {

		//sum이라는 메소드
		//sum(3,5) -> 8이 리턴되게 해주세요
    //매개변수는 3개도 
		
		System.out.println(sum2(3,4,5));
		

	}
	public static int sum(int num1, int num2) {
		return num1+num2;
	}
	
	public static int sum2(int i, int j, int k) {
		return i+j+k;
	}

}
