
public class OOP10 {

	public static void main(String[] args) {

		int[] a = { 4, 3, 1, 2, 5 };
		int b = 5;
		// 배열에 지정된 모든 원소를 출력하시오 ==> arrPrint()
		arrPrint(a); //호출
	}

	// 매개변수로 정수형 배열을 받아서 배열의 모든 원소의 값을 출력하는[메서드를 정의]하시오.
	// 배열을 출력해주는 메소드. 
	public static void arrPrint(int[] x) { //배열을 받을때는 이처럼 넣어주면 된다!

		//향상된 포문
		for(int num : x) {
			System.out.print(num + " ");
		}
		
		// 포문
		/*for (int i = 0; i < 5; i++) {
			System.out.print(x[i] + " ");
		}*/

	}
}
